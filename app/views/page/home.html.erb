    <%if current_user%>
    <%= form_for(@activity) do |f| %>
      <div class="row">
        <div class="col-sm-12">
          <div class="panel panel-default text-left">
            <div class="panel-body">
                <div class="form-group">
                <%= f.text_area :post, autofocus: true, class: "form-control" %>
                </div>
              <%= f.submit "Post", class: "btn btn-primary pull-right"%>
            </div>
          </div>
        </div>
      </div>
      <% end %>
      <%end%>


      <% @activities.order(id: :desc).limit(5).each do |activity| %>
      <div class="row">
        <div class="col-sm-3">
          <div class="well">
           <p><%=link_to activity.user.name, activity.user %></p>

           <%if activity.user.avatar? %>
          <%= image_tag(activity.user.avatar.url(:thumb), class: "img-circle" , height: "65", width: "65 ")%>
         <% else %>
          <%if activity.user.gender == "false"%>
            <%= image_tag("/images/boy.png", class: ".img-rounded", height: "65", width: "65")%>
          <%else%>
            <%= image_tag("/images/girl.png", class: ".img-rounded", height: "65", width: "65")%>
          <%end %>
         <%end%>

           <br>
           <small><%= time_ago_in_words(activity.created_at) %> ago</small>
          </div>
        </div>
        <div class="col-sm-9">
          <div class="well">
            <div class="well">
              <div class="comments_wrapper clearfix">
                <div class="pull-left">
                  <div align="left">
                    <%= activity.post %>
                  </div>
                </div>
            <div class="btn-group pull-right">
            <!-- <div align="right"> -->
            <% if activity.user == current_user -%>
                <%= link_to '', activity, method: :delete, data: { confirm: 'Are you sure?' }, class: "glyphicon glyphicon-trash" %>
            <% end %>
            <!-- </div> -->
            </div>
            </div>
            </div>

            <div align="left">
            <small><%= activity.comments.count%> Comments</small>
            <div id="comments">
            <%= render :partial => activity.comments %>
            </div>
            </div>
           
            <%if current_user%>
            <%= form_for [activity, Comment.new]  do |f| -%>
             <%= f.text_field :body, class: "form-control" %>
            <% end %>
            <%end%>
          </div>
        </div>
      </div>
      <% end %>
   

    </div>
    <% if @event.count != 0%>
    <div class="col-sm-2 well">
      <div class="thumbnail">
        <p>Upcoming Events:</p>
        <%if @event.last.avatar? %>
          <%= image_tag(@event.last.avatar.url(:thumb), class: "img-rounded" , height: "70", width: "70")%>
         <% else %>
           <!--  <%= image_tag("/images/navodayans_small.png", class: "img-rounded", height: "70", width: "70")%> -->
         <%end%>
        <p><strong><%=@event.last.name%></strong></p>
        <p><%= @event.last.when.strftime("%A, %d %b %Y %I:%M%p")%></p>
        <%=link_to "Info", events_path, class:"btn btn-primary"%>
      </div>
      <div class="well">
        <p>ADS</p>
